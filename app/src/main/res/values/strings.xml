<resources>
    <string name="app_name" translatable="false">AppResources</string>
    <string name="hello_world">Hello World!</string>

    <!--
      - %d - шаблон для замены.
      - % - начало шаблона,
      - d - тип передаваемого аргумента
      -->
    <string name="cats_in_house" translatable="false">Cats in the house: %d</string>

    <!-- 1$s - обозначает номер переданного аргумента и его тип -->
    <string name="animal_name" translatable="false">Name of the %1$s is %2$s. %2$s is a %1$s</string>

    <!-- .2f - кол-во цифр после запятой -->
    <string name="cat_weight" translatable="false">The cat weights %.2f kilos</string>

    <!-- t - тип аргумента - время, b,e,Y - флаги специфичные для данного типа -->
    <string name="cats_birthday" translatable="false">Cat\'s birthday is on: %1$tb %1$te %1$tY</string>

    <string name="cat_name_prefix">Cat name is: %s</string>

    <!-- Массив строчек -->
    <array name="cat_names">
        <item>Murzik</item>
        <item>Murka</item>
        <item>Pushok</item>
    </array>

    <!-- Множественное число (plurals), см. русские ресурсы для иллюстрации -->
    <plurals name="cats_quantity">
        <item quantity="one">I have %d cat</item>
        <item quantity="other">I have %d cats</item>
    </plurals>
</resources>